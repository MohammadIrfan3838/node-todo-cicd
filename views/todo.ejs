<html>
    <head>
       <title> Login Form Design </title>
           <link rel="stylesheet" type="text/css" href="style.css">
       <body>
           <div class="loginbox">
           <img src="icon.png" class="icon">
               <h1>Login Here</h1>
               <form>
                   <p>Username </p>
                   <input type="text" name="" placeholder="Enter Username">
                   <p>Password</p>
                   <input type="password" name="" placeholder="Enter password"><br>
                   <input type="submit" name="" value="Login"><br>
                   <a href="#">You can reset your password?</a><br>
                   <a href="#">Don't have account?</a><br>
                </form>
            </div>
        </body>
    </head>
 </html>
 
 <!DOCTYPE html>

<html>

    <head>
        <title>My shaandaar todolist</title>
        <style>
            a {
                text-decoration: none;
                color: black;
            }

        </style>
    </head>

    <body>
        <h1>Jenkins integration is just pretty Awesome</h1>
        <ul>
            <% todolist.forEach(function(todo, index) { %>
            <li>
                <a href="/todo/delete/<%= index %>">✘</a>
                <a href="/todo/<%= index %>">✎</a>
                <%- todo %>
            </li>
            <% }); %>
        </ul>

        <form action="/todo/add/" method="post">
            <p>
                <label for="newtodo">What shoud I do?</label>
                <input type="text" name="newtodo" id="newtodo" autofocus />
                <input type="submit" value="Add" />
            </p>
        </form>
    </body>

</html>





